{
  "name": "communityboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && dotnet run",
    "dev:frontend": "npm run dev:vite",
    "dev:vite": "vite",
    "install:frontend": "npm install",
    "install:backend": "cd backend && dotnet restore",
    "install": "npm run install:backend"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-bootstrap": "^2.10.9",
    "bootstrap": "^5.3.6",
    "sass": "^1.77.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vite-plugin-pwa": "^1.1.0"
  }
}
